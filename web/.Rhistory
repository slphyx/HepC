install.packages("devtools")
devtools::install_github("slphyx/maemod")
library(maemod)
orenz<-"
! MAEMOD_Begin
STARTTIME <- 0
STOPTIME <- 30
DT <- 0.01
!Equations
dx<-sigma*(y-x)
dy<-x*(rho-z)-y
dz<-x*y-beta*z
!Parameters
sigma=10, beta=2.66666, rho=28
!Inits
x=-10, y=-12, z=30.05
!Outputs
!Plots
!MAEMOD_End
"
lorenzout<-maemod.ode(input.text = lorenz)
library(scatterplot3d)
scatterplot3d(lorenzout[,c(2,3,4)],type = 'l')
